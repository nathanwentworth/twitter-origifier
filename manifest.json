{
  "name": "Twitter Origifier",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Makes twitter images their original resolution",
  "homepage_url": "https://nathanwentworth.co",
  "content_scripts": [{
    "matches": ["https://pbs.twimg.com/media/*"],
    "js": ["origifier.js"],
    "run_at": "document_end"
  }],
  "permissions": [
    "activeTab"
  ]
}